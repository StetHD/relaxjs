{
  "name": "relaxjs",
  "description": "A library written in Typescript to build real RESTful webservices in node.js",
  "author": {
    "name": "Michele Ursino",
    "email": "michele@micurs.com",
    "url": "http://github.com/micurs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micurs/relaxjs"
  },
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/relaxjs.js",
  "bugs": "https://github.com/micurs/relaxjs/issues",
  "keywords": [
    "REST",
    "RESTful",
    "webservices",
    "typescript",
    "no routing",
    "no routes",
    "resource",
    "framework"
  ],
  "typescript": {
    "definition": "dist/relaxjs.d.ts"
  },
  "scripts": {
    "build": "tsc -m commonjs -t ES5 --outdir dist --sourcemap --sourceRoot ../src src/routing.ts src/internals.ts src/relaxjs.ts",
    "typedef": "dts-generator --name relaxjs --main relaxjs.ts --baseDir src --out dist/relaxjs.d.ts relaxjs.ts",
    "tsd": "tsd reinstall --save --overwrite",
    "test": "jasmine-node --verbose --color tests"
  },
  "files": [
    "dist/",
    "src/",
    "views/",
    "typings/",
    "tests",
    "readme.md",
    "gulpfile.js",
    "LICENSE",
    "tsd.json"
  ],
  "preferGlobal": "false",
  "dependencies": {
    "bunyan": "^1.3.4",
    "lodash": "^3.3.1",
    "mime": "^1.2.11",
    "multiparty": "^4.1.1",
    "q": "^1.1.2",
    "xml2js": "^0.4.5"
  },
  "devDependencies": {
    "better-console": "^0.2.3",
    "debug": "~2.0.0",
    "gulp": "^3.8.9",
    "gulp-jshint": "^1.8.5",
    "gulp-less": "^1.3.6",
    "gulp-minify-css": "^0.3.10",
    "gulp-print": "^1.1.0",
    "gulp-rename": "^1.2.0",
    "gulp-shell": "^0.4.2",
    "gulp-sourcemaps": "^1.2.2",
    "gulp-tsc": "^0.9.2",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^1.0.7",
    "jasmine": "^2.0.1",
    "jasmine-node": "^1.14.5",
    "jshint-stylish": "^1.0.0",
    "yargs": "^1.3.1"
  }
}
